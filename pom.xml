    <artifactId>terasoluna-gfw-web-blank</artifactId>
    <version>5.5.0-SNAPSHOT</version>
    <packaging>war</packaging>
    <parent>
        <groupId>org.terasoluna.gfw</groupId>
        <artifactId>terasoluna-gfw-parent</artifactId>
        <version>5.5.0-SNAPSHOT</version>
        <relativePath />
